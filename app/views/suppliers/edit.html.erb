<%= form_with url:"/suppliers/#{@supplier.id}", method: :patch, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %></br>

  <%=form.label 'Number of Employees' %>
  <%= form.number_field :number_of_employees %></br>

  <%= form.label 'Open Weekends' %>
  <%= form.check_box :open_weekends %></br>

  <%= form.submit 'Update Supplier' %>
<% end %>
