<%= link_to "Home", "/" %>
<%= link_to "Restaurants and Chefs", "/eats" %>
<%= link_to "Supplier Index", "/suppliers" %>
<%= link_to "Supply Truck Index", "/supply_trucks" %>

<%= form_with url:"/suppliers", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %></br>

  <%=form.label 'Number of Employees' %>
  <%= form.number_field :number_of_employees %></br>

  <%= form.label 'Open Weekends' %>
  <%= form.check_box :open_weekends %></br>

  <%= form.submit 'Create New Supplier' %>
<% end %>
